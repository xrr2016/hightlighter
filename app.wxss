page{
  font-family: 'Avenir', Helvetica, Arial, sans-serif;
  font-size: 28rpx;
  background-color: #E4FCF9;
  color: #f4f4f4;
}
.float-container{
  position: fixed;
  bottom: 40rpx;
  right: 40rpx;
  overflow: hidden;
}
.float-action{
  float: right;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  box-shadow: 2px 2px 10px rgba(0,0,0,.4);
  background-color: #446491;
  z-index: 99;
  text-align: center;
  opacity: .5;
  margin-left: 40rpx;
}
.float-action:active{
  opacity: 1;
}
.float-action image{
  width: 60rpx;
  height: 60rpx;
  margin-top: 20rpx;
}
.bg-normal{
  background-color: #46bc62 !important;
}
.bg-warning{
  background-color: #ecaa5b !important;
}
.bg-danger{
  background-color: #e14848 !important;
}

.color-primary{
  color: #46bc62 !important;
}
.color-warning{
  color: #ecaa58 !important;
}
.color-danger{
  color: #e14848 !important;
}
.border-normal{
  border:2rpx solid #46bc62 !important;
}
.border-warning{
  border: 2rpx solid #ecaa58 !important;
}
.border-danger{
  border:2rpx solid #e14848 !important;
}
.text-center{
  text-align: center !important;
}